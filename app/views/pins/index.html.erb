<div id="pins" class="transitions-enabled">
  <% @pins.each do |pin| %>
    <div class="box panel panel-default">
      <%= link_to (image_tag pin.image.url)%>
      <div class="panel-body"><h2><%= link_to pin.title, pin %></h2></div>
      <p>Submitted by <%= pin.user.email %>
      <p class="small"><%= time_ago_in_words(pin.created_at) %> ago</p>
      </p>
    </div>
  <% end %>
</div>
